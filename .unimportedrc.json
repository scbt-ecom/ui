{
  "preset": "node",
  "entry": ["src/index.ts", "src/*.{ts,tsx}", ".storybook/preview.ts"],
  "extensions": [".js", ".jsx", ".ts", ".tsx"],
  "ignorePatterns": [
    "**/*.tests.{js,jsx,ts,tsx}",
    "**/*.test.{js,jsx,ts,tsx}",
    "**/*.spec.{js,jsx,ts,tsx}",
    "**/tests/**",
    "**/__tests__/**",
    "**/__snapshots__/**",
    "**/*.d.ts",
    "**/*.stories.**",
    "**/src/playroom/*",
    "**/src/storybook/**",
    "**/public/**",
    "**/node_modules/**"
  ],
  "ignoreUnimported": ["src/utils/deepCompare.ts", "src/utils/getFiles.js"],
  "ignoreUnused": [
    "@fontsource/roboto",
    "@radix-ui/react-popover",
    "@radix-ui/react-slider",
    "express",
    "react-hook-form",
    "react-number-format"
  ]
}
