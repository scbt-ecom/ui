{
  "name": "@ecom/ui",
  "version": "0.0.0-development",
  "description": "",
  "scripts": {
    "publish-ui": "npm run publish:prepare && npm run semantic-release",
    "pack-ui": "npm run publish:prepare && cd lib && nano package.json && npm pack",
    "publish:prepare": "npm run build && cp package.json lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "start-storybook -p 3000",
    "start:ci": "start-storybook -p 8080 --ci",
    "build-storybook": "build-storybook",
    "compile": "babel src --out-dir lib --ignore \"**/*.stories.js\" --copy-files --no-copy-ignored",
    "build": "npm run build:prepare && npm run compile",
    "build:prepare": "rm -rf lib",
    "semantic-release": "semantic-release",
    "cz": "cz -a"
  },
  "author": "I",
  "license": "ISC",
  "dependencies": {
    "@date-io/luxon": "^1.3.13",
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.7.1",
    "classnames": "^2.2.6",
    "luxon": "^1.21.3",
    "memoize-one": "^5.1.1",
    "react": "^16.14.0",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.14.0",
    "react-dropzone": "^11.2.4",
    "react-imask": "^6.0.5",
    "react-number-format": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.8",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/node-logger": "^6.1.11",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.11",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-helmet": "^6.1.0",
    "react-scripts": "4.0.1",
    "semantic-release": "^17.3.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
