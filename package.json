{
  "name": "@ecom/ui-beta",
  "version": "0.0.0-development",
  "description": "sovcombank ecom ui library",
  "author": "I",
  "license": "ISC",
  "types": "index.d.ts",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "prebuild:storybook": "npm run test:output || true",
    "build": "rollup -c",
    "build:dev": "cross-env NODE_ENV=development npm run build",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:storybook": "storybook build -o ./public",
    "build:playroom": "playroom build",
    "start": "concurrently -k -s first -n \"Storybook,Playroom,Jest\" -c \"magenta,blue,red\" \"npm run start:storybook\" \"npm run start:playroom\" \"npm run test:output:watch\"",
    "start:storybook": "storybook dev -p 6006 --no-open",
    "start:playroom": "playroom start",
    "lint": "eslint . --max-warnings 0",
    "ts-check": "tsc --noEmit --pretty",
    "test": "jest",
    "test:storybook": "concurrently -k -s first -n \"Storybook,Test-runner\" -c \"magenta,blue\" \"npx http-server ./public -p 8080\" \"wait-on tcp:8080 && test-storybook --url http://127.0.0.1:8080\"",
    "test:ci": "npm run test && npm run test:storybook",
    "test:output": "jest --json --outputFile=.jest-test-results.json",
    "test:output:watch": "npm run test:output -- --watchAll || true",
    "cz": "cz -a",
    "prepublish:ui": "npm run build && cp package.json lib",
    "publish:ui": "npm run semantic-release",
    "semantic-release": "semantic-release",
    "clean": "rm -rf lib public",
    "clean:all": "npm cache clean -f & rm -rf node_modules & npm run clean",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/material-icons": "^5.0.3",
    "@fontsource/roboto": "^5.0.3",
    "@mui/material": "^5.13.4",
    "clsx": "^2.0.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/plugin-url": "^8.0.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^12.0.3",
    "@storybook/addon-a11y": "^7.3.2",
    "@storybook/addon-actions": "^7.0.18",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-jest": "^7.3.2",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/addon-storysource": "^7.0.18",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "^7.0.18",
    "@storybook/jest": "^0.2.1",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.0.18",
    "@storybook/react-webpack5": "^7.0.18",
    "@storybook/test-runner": "^0.13.0",
