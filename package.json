  "browserslist": {
    "production": [
      ">0.5%",
      "last 5 versions",
      "Firefox ESR",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,scss}": "pretty-quick --staged",
    "*.{ts,tsx}": "eslint --fix --max-warnings 0"
  }
}
