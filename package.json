{
  "name": "@ecom/ui",
  "version": "0.0.0-development",
  "description": "",
  "scripts": {
    "publish-ui": "npm run publish:prepare && npm run semantic-release",
    "pack-ui": "npm run publish:prepare && cd lib && nano package.json && npm pack",
    "publish:prepare": "npm run build && cp package.json lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "start-storybook -p 8080",
    "start:ci": "start-storybook -p 8080 --ci",
    "build-storybook": "build-storybook",
    "compile": "NODE_ENV=production babel src --out-dir lib --copy-files --ignore \"**/*.stories.js\" --no-copy-ignored --config-file ./babelConfig.js",
    "build": "npm run build:prepare && npm run compile",
    "build:prepare": "rm -rf lib",
    "lint": "eslint src --ext .js --max-warnings 0",
    "semantic-release": "semantic-release",
    "cz": "cz -a"
  },
  "author": "I",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@date-io/luxon": "^1.3.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.3.10",
    "@material-ui/styles": "^4.11.3",
    "classnames": "^2.2.6",
    "luxon": "^1.26.0",
    "memoize-one": "^5.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.14.0",
    "react-dropzone": "^11.2.4",
    "react-imask": "^6.0.5",
    "react-number-format": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-exponentiation-operator": "^7.12.1",
    "@babel/plugin-transform-react-constant-elements": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.9",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/react": "^6.1.15",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.4.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-helmet": "^6.1.0",
    "semantic-release": "^17.3.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "pretty-quick --staged",
      "eslint --fix --max-warnings 0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 5 versions",
      "Firefox ESR",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
