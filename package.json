    "build:prepare": "rm -rf dist",
    "build": "npm run build:prepare && rollup -c",
    "build-sb": "storybook build",
    "lint": "eslint . --max-warnings 0",
    "ts-check": "tsc --noEmit --pretty",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cz": "cz -a",
